SELECT
    [System.Id],
    [System.WorkItemType],
    [System.Title],
    [System.AssignedTo],
    [System.State],
    [System.Tags]
FROM workitems
WHERE
    [System.TeamProject] = @project
    AND [System.WorkItemType] = 'User Story'
    AND [System.State] <> ''
    AND [System.IterationPath] = @currentIteration('[DC Azure Migration]\DC Azure Migration Team <id:dc215fe1-34ed-4929-ac95-7de1b5daf6df>')
