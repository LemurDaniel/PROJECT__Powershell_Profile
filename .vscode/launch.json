    "version": "0.2.0",
    "configurations": [
        {
            "name": "[TEST] - Import Modules in Temporary Shell",
            "type": "PowerShell",
            "request": "launch",
            "preLaunchTask": "Generate Meta-Files",
            "script": "Import-Module",
            "args": [
                "'${workspaceFolder}/DevOpsScripts'"
            ],
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": true
        }
    ]
}